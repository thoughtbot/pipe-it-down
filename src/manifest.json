{
  "manifest_version": 2,
  "name": "Pipe it Down",
  "description": "Reduce diff noise in GitHub pull requests",
  "version": "0.1.0",
  "author": "Chris Thorn",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "zepto.min.js",
        "github.js",
        "icons.js",
        "store.js",
        "file.js",
        "client.js"
      ]
    }
  ]
}
